{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": true,
		"outDir": "./dist",
		"strict": false,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"importHelpers": true,
		"removeComments": true,
		"preserveConstEnums": true
	}
}
